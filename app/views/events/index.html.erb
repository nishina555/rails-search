<!DOCTYPE html>
<html>
<head>
  <title>Event Index</title>
</head>
<body>
<%= search_form_for(@q, url:events_path,method: :get) do |f| %>
  <div>
    <%= f.label :name_cont %>
    <%= f.search_field :name_cont %>
  </div>
  <div>
    <%= f.label :body_cont %>
    <%= f.search_field :body_cont %>
  </div>
  <%= f.submit %>
<% end %>
<h1>Event List</h1>
<div>
  <% @events.each do |event| %>
    <div>
      <%= event.name %>
      <%= event.body %>
    </div>
  <% end %>
</div>
</body>
</html>